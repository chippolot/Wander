# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

source 'https://github.com/CocoaPods/Specs.git'

use_frameworks!

target 'Wander' do

  # Pods for Wander
  pod 'RxSwiftExt'
  pod 'RxSwift',    '~> 4.0'
  pod 'RxCocoa',    '~> 4.0'
  pod 'Swinject'
  pod 'SwinjectStoryboard'

  target 'WanderTests' do
    inherit! :search_paths

    # Pods for testing
    pod 'RxBlocking', '~> 4.0'
    pod 'RxTest',     '~> 4.0'
    pod 'Spry'        
    pod 'Quick'
    pod 'Nimble'
    pod 'Spry+Nimble'
  end
end
